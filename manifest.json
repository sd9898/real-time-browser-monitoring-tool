{
    "manifest_version": 3,
    "name": "SafeBrowsing Monitor",
    "version": "1.0",
    "description": "Monitors browsing and checks if sites are safe or malicious.",
    "permissions": ["webNavigation", "storage", "activeTab", "notifications"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    },
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    },
    "host_permissions": ["<all_urls>"]
  }
  